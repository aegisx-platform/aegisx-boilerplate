{
  "name": "@aegisx-boilerplate/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "npx nx run-many --target=serve --projects=@aegisx-boilerplate/api,web --parallel",
    "start:api": "npx nx serve @aegisx-boilerplate/api",
    "start:web": "npx nx serve web",
    "build": "npx nx run-many --target=build --projects=@aegisx-boilerplate/api,web --parallel",
    "build:api": "npx nx build @aegisx-boilerplate/api",
    "build:web": "npx nx build web",
    "build:libs": "npx nx run-many --target=build --projects=api-client,types --parallel",
    "test": "npx nx run-many --target=test --all",
    "test:api": "npx nx test @aegisx-boilerplate/api",
    "test:web": "npx nx test web",
    "lint": "npx nx run-many --target=lint --all",
    "lint:api": "npx nx lint @aegisx-boilerplate/api",
    "lint:web": "npx nx lint web",
    "typecheck": "npx nx run-many --target=typecheck --all",
    "clean": "npx nx reset && rm -rf dist tmp",
    "deps:check": "npx nx graph",
    "format": "npx nx format:write",
    "format:check": "npx nx format:check",
    "db:setup": "docker-compose up -d postgres",
    "db:migrate": "npx knex migrate:latest --knexfile knexfile.prod.js",
    "db:rollback": "npx knex migrate:rollback --knexfile knexfile.prod.js",
    "db:seed": "npx knex seed:run --knexfile knexfile.prod.js",
    "db:reset": "npx knex migrate:rollback --all --knexfile knexfile.prod.js && npx knex migrate:latest --knexfile knexfile.prod.js && npx knex seed:run --knexfile knexfile.prod.js",
    "db:status": "npx knex migrate:status --knexfile knexfile.prod.js",
    "db:make:migration": "npx knex migrate:make --knexfile knexfile.prod.js",
    "db:make:seed": "npx knex seed:make --knexfile knexfile.prod.js",
    "db:dev:migrate": "npx knex migrate:latest --knexfile knexfile.ts",
    "db:dev:rollback": "npx knex migrate:rollback --knexfile knexfile.ts",
    "db:dev:seed": "npx knex seed:run --knexfile knexfile.ts",
    "db:dev:status": "npx knex migrate:status --knexfile knexfile.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "logs:seq": "docker-compose -f docker-compose.seq.yml up -d",
    "logs:grafana": "docker-compose -f docker-compose.loki.yml up -d",
    "logs:graylog": "docker-compose -f docker-compose.graylog.yml up -d"
  },
  "private": true,
